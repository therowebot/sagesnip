<script type="text/javascript">function addStructuredData(e){var t=document.createElement("script");t.type="application/ld+json",t.text=JSON.stringify({"@context":"https://schema.org/","@type":"Product",name:e.name,image:[e.image],description:e.description,sku:e.sku,offers:{"@type":"Offer",url:window.location.href,priceCurrency:"USD",price:e.price,availability:"https://schema.org/InStock",seller:{"@type":"Organization",name:"Big Star Branding"}}}),document.querySelector("head").appendChild(t)}function getProductData(){var e={},t=document.querySelector("p.item-numb span.notranslate");return e.name=document.querySelector('meta[property="og:title"]').content,e.description=document.querySelector('meta[property="og:description"]').content,e.image=document.querySelector('meta[property="og:image"]').content,e.sku=t.textContent.replace("Item #","").trim();var r=document.querySelector(".price-grid"),n=r.querySelectorAll("td:last-child");return e.price=n[n.length-1].textContent,e.priceCurrency="USD",e.availability="https://schema.org/InStock",e}function checkProductPage(){var e=window.location.href;e.includes("https://www.bigstarbranding.com/p/")&&addStructuredData(getProductData())}checkProductPage();</script>
